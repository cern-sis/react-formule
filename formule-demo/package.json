{
  "name": "formule-demo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "link-local-lib": "yarn link react-formule && yarn link react && yarn link react-dom",
    "unlink-local-lib": "yarn unlink react-formule && yarn unlink react && yarn unlink react-dom",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headless --browser chrome"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "antd": "^5.7.3",
    "cypress": "^13.6.0",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.45.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^5.0.2"
  }
}
